<fontscolors>

    <fontcolor name="COMMENT_START" foreColor="gray" />
    <fontcolor name="COMMENT" foreColor="gray" />
    <fontcolor name="COMMENT_END" foreColor="gray" />

    <fontcolor name="BLOCK_START" foreColor="blue"><font style="bold" /></fontcolor>
